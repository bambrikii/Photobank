<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.bambrikii.photobank.news.model.NewsLocalized" table="_photobank_news_localized">
		<id column="id" name="id" type="integer">
			<generator class="identity" />
		</id>
		<property column="title" name="title" type="string" />
		<property column="content" name="content" type="string" />
		<!--
		<many-to-one class="org.bambrikii.photobank.news.News"
			column="news_id" name="news" />
		<many-to-one class="org.bambrikii.photobank.model.Language"
			column="language_id" name="language" />
		-->
	</class>
</hibernate-mapping>
