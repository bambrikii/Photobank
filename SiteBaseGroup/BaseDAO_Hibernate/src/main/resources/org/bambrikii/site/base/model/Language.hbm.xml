<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.bambrikii.site.base.model.Language" table="site_languages">
		<id column="site_language_id" name="id" type="integer">
			<generator class="identity" />
		</id>
		<property name="name" column="name" type="string" />
		<property name="active" column="_active" type="boolean" />
		<property name="order" column="_order" type="int" />
		<property name="locale" column="locale" type="string" />
	</class>
</hibernate-mapping>
