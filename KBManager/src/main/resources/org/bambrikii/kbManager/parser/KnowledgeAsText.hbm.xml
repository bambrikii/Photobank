<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<joined-subclass
		name="org.bambrikii.kbManager.parser.model.KnowledgeAsText"
		extends="org.bambrikii.kbManager.parser.model.Knowledge" table="kb_knowledge_as_text">
		<key column="knowledge_id" />
		<property column="title" name="title" type="string" />
		<property column="content" name="content" type="string" />
	</joined-subclass>
</hibernate-mapping>
